{
  "name": "gdbg",
  "version": "0.5.0",
  "description": "Game Debugger & Memory Analysis Tool",
  "main": "index.js",
  "bin": {
    "gdbg": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "build": "pkg . --compress GZip",
    "build-win": "pkg . --targets node16-win-x64 --output dist/gdbg-win.exe --compress GZip",
    "build-macos": "pkg . --targets node16-macos-x64 --output dist/gdbg-macos --compress GZip",
    "build-linux": "pkg . --targets node16-linux-x64 --output dist/gdbg-linux --compress GZip",
    "build-all": "npm run build-win && npm run build-macos && npm run build-linux",
    "compile-agent": "frida-compile frida-agent.js -o gdbg.js -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MswTester/gdbg.git"
  },
  "keywords": [],
  "author": "MswTester",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/MswTester/gdbg/issues"
  },
  "homepage": "https://github.com/MswTester/gdbg#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "frida": "^16.0.8"
  },
  "devDependencies": {
    "frida-compile": "^16.4.2",
    "pkg": "^5.8.0"
  },
  "pkg": {
    "scripts": "index.js",
    "assets": [
      "gdbg.js"
    ],
    "targets": [
      "node16-win-x64",
      "node16-macos-x64",
      "node16-linux-x64"
    ],
    "outputPath": "dist"
  }
}
